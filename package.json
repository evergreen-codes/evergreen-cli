{
  "name": "evrgrn",
  "description": "Keep your dependencies up to date",
  "version": "0.0.3",
  "author": "Jacob Friedmann",
  "bin": {
    "evergreen": "./bin/run"
  },
  "bugs": "https://github.com/evergreen-codes/evergreen-cli/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.3.2",
    "@octokit/graphql": "^4.8.0",
    "@snyk/cocoapods-lockfile-parser": "^3.6.2",
    "@yarnpkg/lockfile": "^1.1.0",
    "csv-writer": "^1.6.0",
    "depcheck": "^1.4.2",
    "glob": "^7.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "node-fetch": "^2.6.6",
    "node-html-parser": "^5.2.0",
    "query-registry": "^2.2.0",
    "read-package-json": "^4.1.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/node-fetch": "^3.0.3",
    "@types/yarnpkg__lockfile": "^1.1.5",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^1.7.45",
    "prettier": "^2.5.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "bin/**/*",
    "dist/**/*",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/evergreen-codes/evergreen-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "evergreen-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
